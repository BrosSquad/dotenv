{
    "name": "dusan/php-mvc-dotenv",
    "description": "Env Parser for PhpMvc Framework",
    "authors": [
        {
            "name": "Dusan Malusev",
            "email": "dusan.998@outlook.com"
        }
    ],
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {
            "Dusan\\PhpMvc\\Env\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["src/functions.php"],
        "psr-4": {
            "Dusan\\PhpMvc\\Tests\\Env\\": "tests/"
        }
    },
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "dusan/php-mvc-file": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "license": "GPL-2.0",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/malusev998/PhpMvc-File.git",
            "branch": "master"
        }
    ]
}
